        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" TextAlignment="Center" Text="Setting Goals &amp; Starting a Healthy Eating" FontSize="48" FontFamily="Arial" Foreground="#FF21BD9F" FontWeight="Bold"/>
  
    </Grid>
</UserControl>
